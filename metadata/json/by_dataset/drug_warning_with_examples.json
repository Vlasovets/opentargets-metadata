        "ref_id": {
          "name": "ref_id",
          "data_type": "Text",
          "description": "List of identifiers for the reference sources",
          "example": "de109a2b-e36c-40d0-85fc-a67a9e7f1ae8"
        },
        "ref_type": {
          "name": "ref_type",
          "data_type": "Text",
