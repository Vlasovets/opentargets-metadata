{
  "datasets": {
    "drug_warning": {
      "name": "drug_warning",
      "columns": {
        "chemblIds": {
          "name": "chemblIds",
          "data_type": "Array[Text]",
          "description": "List of Open Targets molecule identifiers",
          "example": [
            "CHEMBL4303288",
            "CHEMBL1380"
          ]
        },
        "toxicityClass": {
          "name": "toxicityClass",
          "data_type": "Text",
          "description": "Classification of toxicity type associated with the drug",
          "example": "hepatotoxicity"
        },
        "country": {
          "name": "country",
          "data_type": "Text",
          "description": "Country where the warning was issued",
          "example": "United States"
        },
        "description": {
          "name": "description",
          "data_type": "Text",
          "description": "Description of the drug adverse effect",
          "example": "Nephropathy"
        },
        "id": {
          "name": "id",
          "data_type": "Float",
          "description": "Internal identifier for the drug warning record",
          "example": 1
        },
        "references": {
